    background-color: #323131;
    color: #fff;  background-size:60% auto;
    background-position: calc(100% + 50px) 30px;
}
body#default.dark header .title {
    color: #fff;
}
body#default.dark #content {
    padding-top: 300px;
}
body#default.fill>#frame {
    background-color: #fff;
}
/* after loading */
body#default.dark header img, body#default.dark.full.on header img {
    width: 84px;margin-top: 0;
}
body#default.dark.on header {
    height: 260px;
}
body#default.on header .title {
    opacity: 1;margin-top: 20px;
}
body#default.dark.on #content {
    padding-top:260px;
}
body#default.on #content {
