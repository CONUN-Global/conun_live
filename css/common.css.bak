/* . */
/* receipt */
body.blur {
    overflow-y: hidden;
}
body.blur>div>*:not(#receipt){
    filter: blur(1.15rem);
    -webkit-filter: blur(1.15rem);
    -moz-filter: blur(1.15rem);
    -o-filter: blur(1.15rem);
    -ms-filter: blur(1.15rem);
}
#receipt {
    position: fixed;
    left: 0;top: 0;width: 100%;height: 100%;
    background-color:rgba(0,0,0,.6);
    z-index: 10000;
    padding:26px;
    overflow-y: auto;
}
#receipt .inner {
    position: relative;
    z-index: 10000;
}
#receipt #header, #receipt #cont {
    background-color: #fff;
    padding-left: 30px;padding-right: 30px;
}
#receipt #header {
    padding-top: 30px;
}
#receipt #mid {
    height: 60px;
}
#receipt #mid>div {
    width: 15px;
    background-size:auto 100%;
    background-repeat:no-repeat;
    background-position: left top;
}
#receipt #mid>div:nth-of-type(2){
    width:calc(100% - 30px);
    background-color: #fff;
    background-repeat:repeat-x;
}
#receipt #cont {
    padding-bottom: 50px;
}
#receipt button.active {
    border-top-left-radius: 0;
    border-top-right-radius: 0;
}
#receipt span.type {
    color: #3D455C;
    display: inline-block;
    margin-right: 8px;padding-right: 8px;
    border-right: 1px solid #999;
}
#receipt[type="out"] span.type {
    color: #BD1213;
}
#receipt.modal {
    background-color:transparent;
}
#receipt.modal .modalbg {
    position: fixed;
}
#receipt.modal .title {
    margin-bottom: 0;
}
#receipt.modal .orderinfo {
    padding-top: 0;
}
#receipt.modal #trans_detail {
    display: none;
}
#receipt #trans_detail {
    margin-bottom: 20px;padding-bottom: 20px;
    border-bottom: 1px solid #dfdfdf;
}
