/* . */
/* history */
input[name="history_filter"]+label {
    padding-bottom:20px;
    text-align: center;
    position: relative;
}
input[name="history_filter"]+label::after {
    height: 2px;
    width: 0px;
    left: 0;bottom: 0;
    background-color: #c3ab41;position: absolute;
    content: '';
    transition:all .4s cubic-bezier(0.18, 0.89, 0.52, 1.24);
    -webkit-transition:all .4s cubic-bezier(0.18, 0.89, 0.52, 1.24);
    -moz-transition:all .4s cubic-bezier(0.18, 0.89, 0.52, 1.24);
}
input[name="history_filter"]:checked+label {
    color: #c3ab41;
}
input[name="history_filter"]:checked+label::after {
    width: 100%;
}
#history_filter {
    width: 100%;max-width: 640px;
    margin:0 auto;
    padding:0 26px 26px 26px;
    position: fixed;z-index: 1001;
    background:linear-gradient(180deg, #fff 55%, transparent);
}
#history_date_filter, #history_type_filter {
    border-radius: 4px;
    height: 50px;
    background-color: #F9F9F8;
    text-align: center;
    border:none;
}
#history_date_filter {
    font-size: 14px;
    width: calc(100% - 60px);
}
#history_type_filter {
    width: 50px;
}
#history_list {
    padding-top: 70px;
}
#history_list table.default tr>td>div.flex {
    align-items: flex-start;
    -webkit-align-items: flex-start;
}
