/* flex */
.flex {
    display:flex;display:-webkit-flex;display:-ms-flexbox;
}
.flex.row {
    flex-direction:row;-webkit-flex-direction:row;-ms-flex-direction:row;
}
.flex.col {
    flex-direction:column;-webkit-flex-direction:column;-ms-flex-direction:column;
}
.flex.justify {
    justify-content: space-between;
    -webkit-justify-content: space-between;
    -ms-flex-pack: justify;
}
/* transition */
.snap {
    transition:all 1s cubic-bezier(0.18, 0.89, 0.52, 1.24);
    -webkit-transition:all 1s cubic-bezier(0.18, 0.89, 0.52, 1.24);
    -moz-transition:all 1s cubic-bezier(0.18, 0.89, 0.52, 1.24);
}
.snap2 {
    transition:all .4s cubic-bezier(0.18, 0.89, 0.52, 1.24);
    -webkit-transition:all .4s cubic-bezier(0.18, 0.89, 0.52, 1.24);
    -moz-transition:all .4s cubic-bezier(0.18, 0.89, 0.52, 1.24);
}
.trans015, span.checkbox::before {
    transition : all .15s ease-in-out;
    -webkit-transition : all .15s ease-in-out;
    -moz-transition : all .15s ease-in-out;
}
/* common */
body, nav, header {
    max-width: 640px;
    margin:0 auto;
}
#frame {
    min-width: 320px;
    width: 100%;
    background-color: #fff;
    overflow-x: hidden;
}
div.wrap {
    padding:26px;
    width: 100%;
}
div.sidewrap{
    padding:0 26px;
    width: 100%;
}
.title {
    font-size: 21px;
    font-weight: bold;letter-spacing: 3px;
    text-transform: uppercase;
    overflow-y: hidden;
    height: auto;
}
.title_min {
    font-size: 14px;
    opacity: .6;font-weight: 300;
}
.title_min.dark {
    opacity: 1;font-weight: 600;
}
.title_nano {
    font-size: 11px;
    opacity: .6;font-weight: 300;
}
.coin {
    font-size: 14px;
    font-weight: 500;
    letter-spacing: .5px;
}
.v_bar {
    background-color: #dfdfdf;
    width: 1px;
}
.h_bar {
    background-color: #dfdfdf;
    height: 1px;
}
header,nav {
    position: fixed;background-color: #fff;
    width: 100%;z-index: 1000;left:0;right:0;
}
header {
    top: 0;background-repeat:no-repeat;
}
header>div.wrap {
    padding-top: 40px;
}
header a#logo {
    display: block;
    width: 128px;height:auto;margin:0 auto;
}
nav {
    bottom: 0;
}
nav>div.wrap {
    padding:20px 26px;
}
nav a{
    position: relative;
    width: 25px;cursor: pointer;
    background-size: 100% auto;
    background-position: center top;
    background-repeat:no-repeat;
}
nav a::after {
    content: '';
    display: block;width: 100%;padding-bottom: 100%;
}

nav a#ico_home {
    background-image:url('../img/icons/ico_home.png');
}
nav a#ico_home.on {
    background-image:url('../img/icons/ico_home_active.png');
}
nav a#ico_my {
    background-image:url('../img/icons/ico_mypage.png');
}
nav a#ico_my.on {
    background-image:url('../img/icons/ico_mypage_active.png');
}
nav a#ico_log {
    background-image:url('../img/icons/ico_history.png');
}
nav a#ico_log.on {
    background-image:url('../img/icons/ico_history_active.png');
}
nav a#ico_contact {
    background-image:url('../img/icons/ico_contact.png');
}
nav a#ico_setting {
    background-image:url('../img/icons/ico_setting.png');
}
nav a#ico_setting.on {
    background-image:url('../img/icons/ico_setting_active.png');
}
/* . */
/* home */
body#home #content>div {
    opacity: 0;
}
body#home #content>div:first-of-type {
    margin-top: 10px;
}
body#home #content>div:nth-of-type(2) {
    margin-top: 20px;
}
body#home #content>div:last-of-type {
    margin-top: 40px;
}
body#home.on #content>div{
    opacity: 1;
    margin-top: 0 !important;
}
#home_nav {
    padding-top: 14px;padding-bottom: 10px;
    margin-bottom: 20px;
}
.ico_menu {
    display: block;
    width: 25%;flex-grow: 1;
    text-align: center;
    padding-top: 30px;
    background-size: auto 25px;
    background-position: center top;
    background-repeat:no-repeat;
}
.ico_menu>span {
    font-size: 11px;letter-spacing: 2px;
    opacity: .6;font-weight: 400;
}
a#ico_pay {
    background-image:url('../img/icons/ico_pay.png');
}
a#ico_addr {
    background-image:url('../img/icons/ico_addr.png');
}
a#ico_send {
    background-image:url('../img/icons/ico_send.png');
}
a#ico_change {
    background-image:url('../img/icons/ico_change.png');
}
#myconwrap {
    padding-left: 26px;
}
#nowcon, #mycon {
    padding:26px 26px 26px 30px;
}
#nowcon>*, #nowcon>*>*, #mycon>* {
    color: #fff !important;
}
#nowcon {
    background-color: #51515a;
    align-items: center;
    -webkit-align-items:center;
}
#mycon {
    background-color: #C3AB41;
    background-image:url('../img/symbol_big_w.png');
    background-size: auto 320px;
    background-repeat:no-repeat;
    background-position: top right;
    position: relative;
}
button#refreshcon {
    width: 17px;
    height: 14px;
    background-color: transparent;
    background-image:url('../img/icons/ico_refresh.png');
    background-size: 100% auto;
    background-repeat:no-repeat;
    background-position: center right;
    /* position: absolute;
    right: 26px;top: 26px; */
}
#mycon #con {
    margin-top: 40px;
    font-size: 29px;
    font-weight: 400;
    letter-spacing: .5px;
}
#mycon #convalue {
    font-size: 18px;
    opacity: .6;
    letter-spacing: 1px;
}
#coins {
    text-align: center;
    margin-bottom: 80px;
}
#coins>.flex {
    padding:20px 0;
}
#coins>.flex .coin {
    font-size: 18px;
}
#coins>.flex>div:first-of-type,#coins>.flex>div:last-of-type {
    width: calc(50% - .5px);
    padding:20px 0;
}
#coins>.flex>div>.coin {
    margin-top: 20px;
}
#coins>p {
    border-top: 1px solid #dfdfdf;
    padding:20px 0;
}
#coins>p>.coin {
    margin-right: 5px;
}
/* body#default */
