* { margin: 0; padding: 0; box-sizing: border-box; font-family: 'Montserrat', 'Noto Sans JP', 'Nanum Gothic', verdana, sans-serif; font-weight: 200; -webkit-font-smoothing: subpixel-antialiased !important; color:#393944;}
html {height: 100%; }
body {position: relative; width: 100%; height:auto; font-size: 14px; line-height: 1.5; word-break: keep-all; background-color:#fff;}
article, aside, details, figcaption, figure, footer, header, hgroup, main, menu, nav, section, summary { display: block; }
a { color: inherit; text-decoration: none; outline: 0; -webkit-tap-highlight-color:rgba(0,0,0,0); }
em { font-style: normal; }
ul { list-style: none; }
h3 { font-size: 17px; font-family: 'Monserrat', 'Nanum Gothic Bold', sans-serif; font-weight: 400; }
h3 > span { margin-left: 6px; display: inline-block; color: #999; font-size: 13px; font-weight: 400; }
fieldset { margin: 0; padding: 0; border: 0; }
caption, legend { color: transparent; font: 0px/0 arial; text-shadow: none; }
input { border-radius: 0; font-size: 13px; -webkit-appearance: none; outline: 0; }
select { border-radius: 0; font-size: 13px; -webkit-appearance: none; outline: 0; cursor: pointer;}
input[type="checkbox"], input[type="radio"] {
    display: none;
}
::-ms-clear { width: 0; height: 0; }
::-ms-reveal { width: 0; height: 0; }
input::-webkit-input-placeholder { color: #aaa; }
input::-moz-placeholder { color: #aaa; }
input:-moz-placeholder { color: #aaa; }
input:-ms-input-placeholder { color: #aaa; }
button { border-radius: 0; border:none;font-size: 13px; -webkit-tap-highlight-color:rgba(0,0,0,0); -webkit-appearance: none; outline: 0; cursor: pointer;    text-transform: none; text-indent: 0px;text-shadow: none; }
.ir { color: transparent; font: 0px/0 arial !important; text-shadow: none; }
pre { white-space: pre-wrap; }
select::-ms-expand {
    display: none;
}
