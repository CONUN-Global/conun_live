/* modal */
.modal, .modalbg {
    width: 100%;height: 100%;
    left: 0;top: 0;
}
.modal {
    position: fixed;
    z-index: 100000;
    display: none;
}
.modalbg {
    background-color: rgba(0,0,0,0.11);
    position: absolute;
    z-index: 0;
}
.modal.show {
    display: block;
}
.modal>div.item {
    z-index: 100;
    width: calc(100% - 52px);
    background-color: #fff;
    border-radius: 2px;
    padding:30px;
    position: absolute;
    top: 50%;
    left: 26px;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
}
.modal .title {
    margin-bottom: 30px;
}
.modal form {
    margin-bottom: 0;
}
.modal input[type="radio"]+label {
    display: block;
    font-size: 16px;
    margin-bottom: 20px;
}
.modal input[type="radio"]:checked+label{
    font-weight: bold;
    color: #c3ab41;
}
.modal input[type="radio"]:checked+label>span.radio{
    border-color: #c3ab41;
}
.modal input[type="radio"]:checked+label>span.radio::after{
    content: '';
}
.modal button {
    position: absolute;
}
.modal button[name="close"] {
    right: 30px;top: 30px;
    background-image: url(../img/icons/ico_close.png);
    background-size: inherit;
    background-repeat:no-repeat;
    background-position: center;
    width: 20px;height:20px;
    background-color: #fff;
    z-index: 100000;
}
.modal button.default {
    left:0;top:calc(100% - 2px);
    border-top-left-radius: 0;border-top-right-radius: 0;
}
span.radio {
    display: inline-block;
    width: 16px;height: 16px;
    border-radius: 12px;
    border:1px solid #aaa;
    position: relative;
    vertical-align: -2px;
    margin-right: 10px;
}
span.radio::after {
    position: absolute;
    left:0;right:0;top:0;bottom:0;margin:auto;
    width: 8px;height: 8px;border-radius: 8px;background-color: #c3ab41;
}
